<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kofhearts.safe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presenter.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presenter.ChangePasswordActivity" />
        <activity android:name=".presenter.EntryListActivity" />
        <activity android:name=".presenter.EntryViewActivity" />
        <activity android:name=".presenter.NewEntryActivity" />
        <activity android:name=".presenter.EditEntryActivity"></activity>
    </application>

</manifest>